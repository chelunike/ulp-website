---
import Competicion from '../../components/plantilla-compe/Competicion.astro';

const titulo = "VALORANT";
const ex_link = "";
const reg_link = "https://drive.google.com/file/d/1CfceGvfOEMl5jpvdwdjWDJtVmQGRAfKu/view?usp=sharing";
const form_link = "https://forms.gle/ZDj36k89vWCiXSBs8";
const img_path = "/assets/backgrounds/valorant.png";
const desc = "Enfréntate en el campo de batalla táctico de VALORANT, donde precisión, estrategia y habilidad determinarán el equipo vencedor. ¡Selecciona a tu agente y demuestra tu destreza en este emocionante shooter!";
const aula = "A2 + B1";
const horario = "14:00h";

---

<Competicion titulo={titulo} descripcion={desc} aula={aula} horario={horario}
    ex_link={ex_link} reg_link={reg_link} form_link={form_link} img_path={img_path} >
</Competicion>